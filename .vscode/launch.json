{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name":"Launch Go",
            "type":"go",
            "request": "launch",
            "mode": "debug",
            "preLaunchTask": "swagger",
            "cwd":"${workspaceFolder}",
            "program": "./main.go",
            "env":{
                "UBLSTORE_PORT":"2050",
                "AWS_ACCESS_KEY_ID":"minioadmin",
                "AWS_SECRET_ACCESS_KEY":"minioadmin",
                "AWS_REGION":"us-east-1",
                "UBLSTORE_STORAGE_S3_UBLPATH": "/ubl",
                "UBLSTORE_STORAGE_S3_ATTACHMENTPATH": "/attachment",
                "UBLSTORE_STORAGE_S3_ENDPOINT": "http://localhost:9000",
                "UBLSTORE_STORAGE_S3_BUCKET": "ubl-store"
            },
            "serverReadyAction":{
                "action": "openExternally",
                "pattern": "http server started on \\[::\\]:([0-9]+)",
                "uriFormat": "http://localhost:%s/swagger/index.html"
            }
        },
        {
            "name":"Launch Go Local Storage",
            "type":"go",
            "request": "launch",
            "mode": "debug",
            "preLaunchTask": "swagger",
            "cwd":"${workspaceFolder}",
            "program": "./main.go",
            "env":{
                "UBLSTORE_PORT":"2050",
                "UBLSTORE_STORAGE_FILESYSTEM_UBLPATH": "./.data2/ubl",
                "UBLSTORE_STORAGE_FILESYSTEM_ATTACHMENTPATH": "./.data2/attachment"
            },
            "serverReadyAction":{
                "action": "openExternally",
                "pattern": "http server started on \\[::\\]:([0-9]+)",
                "uriFormat": "http://localhost:%s/swagger/index.html"
            }
        }
    ]
}